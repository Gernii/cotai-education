{
	"name": "web",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": " vite dev",
		"preview.dev": "wrangler pages dev .svelte-kit/cloudflare",
		"preview.prod": "wrangler pages prod .svelte-kit/cloudflare",
		"build": "pnpm run postinstall && vite build",
		"test": "pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:integration": "playwright test",
		"test:unit": "vitest run",
		"---------FMT FIX-----------": "Fmt check and fix vaid code",
		"fmt.fix": "run-s \"fmt.fix.*\"",
		"fmt.fix.prettier": "prettier --write \"src/**/*.svelte\"",
		"fmt.fix.biome": "biome format --write ./src",
		"---------FMT CHECK-----------": "Fmt check vaid code only",
		"fmt.check": "run-s \"fmt.check.*\"",
		"fmt.check.biome": "biome format ./src",
		"---------LINT FIX-----------": "Lint check and fix vaid code",
		"lint.fix": "run-s \"lint.fix.*\"",
		"lint.fix.eslint": "eslint \"src/**/*.svelte\" --fix",
		"lint.fix.biome": "biome check --write --unsafe ./src",
		"---------LINT CHECK-----------": "Lint check vaid code only",
		"lint.check": "run-s \"lint.check.*\"",
		"lint.check.eslint": "eslint  \"src/**/*.svelte\"  --max-warnings=0",
		"lint.check.biome": "biome check ./src",
		"preview": "wrangler pages dev .svelte-kit/cloudflare",
		"postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/libs/i18n/messages",
		"postbuild": "node scripts/post-build.js"
	},
	"devDependencies": {
		"@fontsource/source-code-pro": "^5.0.18",
		"@iconify-json/devicon": "^1.1.44",
		"@iconify-json/flag": "^1.1.53",
		"@iconify-json/heroicons": "^1.1.21",
		"@iconify-json/hugeicons": "^1.1.0",
		"@iconify-json/logos": "^1.1.43",
		"@iconify-json/lucide": "^1.1.194",
		"@iconify-json/simple-icons": "^1.1.109",
		"@iconify-json/teenyicons": "^1.1.9",
		"@inlang/paraglide-js": "^1.10.1",
		"@inlang/paraglide-js-adapter-sveltekit": "^0.6.7",
		"@isaacs/ttlcache": "^1.4.1",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.81.0",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.44.1",
		"@repo/types-bridge": "workspace:*",
		"@sveltejs/adapter-cloudflare": "^4.5.0",
		"@sveltejs/kit": "^2.5.17",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/typography": "^0.5.13",
		"@tiptap/core": "^2.1.13",
		"@tiptap/extension-document": "^2.1.13",
		"@tiptap/extension-paragraph": "^2.1.13",
		"@tiptap/extension-text": "^2.1.13",
		"@tiptap/pm": "^2.1.13",
		"@tiptap/starter-kit": "^2.1.13",
		"@types/eslint": "8.56.10",
		"@types/markdown-it": "^13.0.7",
		"@types/markdown-it-container": "^2.0.10",
		"@typescript-eslint/eslint-plugin": "^7.13.1",
		"@typescript-eslint/parser": "^7.13.1",
		"autoprefixer": "^10.4.19",
		"cva": "npm:class-variance-authority@^0.7.0",
		"daisyui": "^4.12.2",
		"echarts": "^5.5.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-svelte": "^2.40.0",
		"eslint-plugin-unused-imports": "^3.2.0",
		"iso-639-1": "^3.1.2",
		"keen-slider": "^6.8.6",
		"markdown-it": "^14.1.0",
		"markdown-it-container": "^4.0.0",
		"markdown-it-mark": "^4.0.0",
		"markdown-it-mathjax3": "^4.3.2",
		"markdown-it-merge-cells": "^2.0.0",
		"mdsvex": "^0.11.2",
		"postcss": "^8.4.38",
		"posthog-js": "^1.139.8",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"schema-dts": "^1.1.2",
		"string-strip-html": "^13.4.8",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.1",
		"svelte-inview": "^4.0.2",
		"svelte-sitemap": "^2.6.0",
		"svelte-sonner": "^0.3.24",
		"sveltekit-superforms": "^2.16.1",
		"tailwind-merge": "^2.3.0",
		"tailwindcss": "^3.4.4",
		"tailwindcss-animated": "^1.1.2",
		"tslib": "^2.6.3",
		"unplugin-fonts": "^1.1.1",
		"unplugin-icons": "^0.19.0",
		"valibot": "^0.36.0",
		"vite": "^5.3.1",
		"vite-imagetools": "^7.0.2",
		"vite-plugin-lqip": "^0.0.5",
		"vitest": "^1.0.0",
		"wrangler": "^3.61.0"
	},
	"lint-staged": {
		"src/**/*.svelte": [
			"eslint --fix",
			"eslint --max-warnings=0",
			"prettier --write",
			"prettier --check"
		],
		"src/**/*.ts": [
			"biome format --write --no-errors-on-unmatched",
			"biome check --write --unsafe --no-errors-on-unmatched"
		]
	},
	"type": "module"
}
